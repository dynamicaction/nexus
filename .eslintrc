{
    "env": {
      "es6": true,
      "mocha": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017
    },
    "rules": {
      "curly": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "max-len": [
        2,
        {
          "code": 200,
          "ignoreComments": true
        }
      ],
      "new-cap": 2,
      "no-bitwise": 2,
      "no-caller": 2,
      "no-console": 0,
      "no-sparse-arrays": 0,
      "no-trailing-spaces": 2,
      "no-undef": 2,
      "no-unused-vars": 2,
      "no-use-before-define": 0,
      "quotes": [
        2,
        "single"
      ],
      "strict": 0
    }
  }